@model IEnumerable<Assignment1.Models.FootBallLeague>

<table class="table table-hover" style="margin-top:10%;">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MatchID)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.WinningTeam)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MatchID)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.WinningTeam)
        </td>
       
    </tr>
}

</table>
